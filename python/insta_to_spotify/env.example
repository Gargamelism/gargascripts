# Copy to .env and fill in values

# ACRCloud
ACRCLOUD_HOST=identify-xx-xxxxx.acrcloud.com
ACRCLOUD_ACCESS_KEY=your_key
ACRCLOUD_ACCESS_SECRET=your_secret

# Spotify
SPOTIPY_CLIENT_ID=your_spotify_client_id
SPOTIPY_CLIENT_SECRET=your_spotify_client_secret
SPOTIPY_REDIRECT_URI=http://localhost:8888/callback

# Optional Instagram cookies (choose ONE approach)
# Path to Netscape cookie file:
# IG_COOKIEFILE=/absolute/path/to/instagram_cookies.txt
# OR auto-import from a browser you use on this machine (yt-dlp supported names: chrome, firefox, brave, edge):
# IG_COOKIES_FROM_BROWSER=chrome
